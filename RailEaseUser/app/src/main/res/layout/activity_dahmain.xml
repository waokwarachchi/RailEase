<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:background="@color/white"
    tools:context=".Dahmain">

    <FrameLayout
            android:layout_width="match_parent"
            android:layout_height="686dp"
            android:layout_weight="1"

            android:id="@+id/fram_layout"/>

        <com.etebarian.meowbottomnavigation.MeowBottomNavigation
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:id="@+id/bottom_navigation"

                app:mbn_circleColor="@color/colorPrimaryLight"
                app:mbn_selectedIconColor="@color/colorPrimaryDark"
                app:mbn_countTextColor="@color/white"
                app:mbn_countBackgroundColor="@color/JacksonsPurple"
                app:mbn_defaultIconColor="@color/white"
                app:mbn_backgroundBottomColor="@color/lavender"/>




        </LinearLayout>







